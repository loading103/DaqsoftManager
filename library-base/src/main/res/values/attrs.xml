<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <!--QMUILoading 自定义属性-->
    <declare-styleable name="QMUILoadingView">
        <attr name="qmui_loading_view_size" format="dimension"/>
        <attr name="android:color"/>
    </declare-styleable>
    <attr name="QMUILoadingStyle" format="reference"/>

    <!--    TagFlowLayout-->
    <declare-styleable name="TagFlowLayout" tools:ignore="ResourceName">
        <attr name="max_select" format="integer" />
        <attr name="tag_gravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>

</resources>